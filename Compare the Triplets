#include<stdio.h>
int main()
{
    int i,j,a[100],b[100],k,n=0,m=0;
    for(i=0;i<3;i++)
    scanf("%d",&a[i]);
    for(j=0;j<3;j++)
    scanf("%d",&b[j]);
    for(k=0;k<3;k++)
    {
    if(a[k]>b[k])
    {
       m+=1; 
        
    }
    else if(a[k]<b[k])
    {
      n+=1;
    }
    else 
    {
      m+=0;
      n+=0;
    }
      }
      printf("%d %d",m,n);
      return 0;
}


